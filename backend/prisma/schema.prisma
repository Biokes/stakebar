generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  email String @unique
  id Int @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  pin String?
}